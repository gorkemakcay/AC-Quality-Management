	<script>

		$("#AddRevisionModal").on('show.bs.modal', function() {
			document.getElementById('frmAddRevision').reset();
		});

		$("#btnAddRevision").click(function() {
			let model = {
				RevisionDate: $("#addRevisionDate").val(),
				RevisionExplain: $("#addRevisionExplain").val(),
				ReasonForRevision: $("#addReasonForRevision").val(),
				IntendedPurpose: $("#addRevisionPurpose").val(),
				RevisionResult: $("#addRevisionResult").val(),
				RevisionType: $("#addRevisionType").val(),
				RevisionRequestedBy: $("#addRevisionRequestBy").val(),
				FinalQualityId: $("#addRevisionQId").val()
			};

			$.ajax({

				type: "POST",
				url: '@Url.Action("AddRevisionJSON", "Revision")',
				data: { model: model },
				success: function(data) {
					specModel = jQuery.parseJSON(data);
					window.location.reload();

				},
				error: function(error) {
				}
			});
		});

		$("#UpdateRevisionModal").on('show.bs.modal', function(e) {
			document.getElementById('frmUpdateRevision').reset();
			let revisionId = $(e.relatedTarget).data('revision-id');
			$.ajax({

				type: "POST",
				url: '@Url.Action("GetRevisionInfo", "Revision")',
				data: { revisionId: revisionId },
				success: function(data) {
					revisionModel = jQuery.parseJSON(data);
					//window.location.reload();
					let model = {
						Id: revisionModel.Id,
						RevisionDate: revisionModel.RevisionDate,
						RevisionExplain: revisionModel.RevisionExplain,
						ReasonForRevision: revisionModel.ReasonForRevision,
						IntendedPurpose: revisionModel.IntendedPurpose,
						RevisionResult: revisionModel.RevisionResult,
						RevisionType: revisionModel.RevisionType,
						RevisionRequestedBy: revisionModel.RevisionRequestedBy,
						FinalQualityId: revisionModel.QualityTableId
					};

					$("#updateName").val(model.RevisionExplain),
						$("#updateRevisionDate").val(model.RevisionDate),
						$("#updateRevisionExplain").val(model.RevisionExplain),
						$("#updateReasonForRevision").val(model.ReasonForRevision),
						$("#updateRevisionPurpose").val(model.IntendedPurpose),
						$("#updateRevisionResult").val(model.RevisionResult),
						$("#updateRevisionType").val(model.RevisionType),
						$("#updateRevisionRequestBy").val(model.RevisionRequestedBy),
						$("#updateRevisionQId").val(model.QualityTableId)



					$("#btnUpdateRevision").click(function() {

						let updatedModel = {
							Id: revisionModel.Id,
							RevisionDate: $("#updateRevisionDate").val(),
							RevisionExplain: $("#updateRevisionExplain").val(),
							ReasonForRevision: $("#updateReasonForRevision").val(),
							IntendedPurpose: $("#updateRevisionPurpose").val(),
							RevisionResult: $("#updateRevisionResult").val(),
							RevisionType: $("#updateRevisionType").val(),
							RevisionRequestedBy: $("#updateRevisionRequestBy").val(),
							FinalQualityId: $("#updateRevisionQId").val()
						};


						$.ajax({
							type: "POST",
							url: '@Url.Action("UpdateRevisionJSON", "Revision")',
							data: { model: updatedModel },
							success: function(data) {
								rModel = jQuery.parseJSON(data);
								window.location.reload();
							},
							error: function(error) {
							}
						});
					});
				},
				error: function(error) {
				}
			});
		});
	</script>

	<!-- #region Text area size and animated -->

	<script>
		(function($) {
			var
				defaults = {
					className: 'autosizejs',
					append: '',
					callback: false,
					resizeDelay: 10
				},

				// border:0 is unnecessary, but avoids a bug in Firefox on OSX
				copy = '<textarea tabindex="-1" style="position:absolute; top:-999px; left:0; right:auto; bottom:auto; border:0; padding: 0; -moz-box-sizing:content-box; -webkit-box-sizing:content-box; box-sizing:content-box; word-wrap:break-word; height:0 !important; min-height:0 !important; overflow:hidden; transition:none; -webkit-transition:none; -moz-transition:none;"/>',

				// line-height is conditionally included because IE7/IE8/old Opera do not return the correct value.
				typographyStyles = [
					'fontFamily',
					'fontSize',
					'fontWeight',
					'fontStyle',
					'letterSpacing',
					'textTransform',
					'wordSpacing',
					'textIndent'
				],

				// to keep track which textarea is being mirrored when adjust() is called.
				mirrored,

				// the mirror element, which is used to calculate what size the mirrored element should be.
				mirror = $(copy).data('autosize', true)[0];

			// test that line-height can be accurately copied.
			mirror.style.lineHeight = '99px';
			if ($(mirror).css('lineHeight') === '99px') {
				typographyStyles.push('lineHeight');
			}
			mirror.style.lineHeight = '';

			$.fn.autosize = function(options) {
				if (!this.length) {
					return this;
				}

				options = $.extend({}, defaults, options || {});

				if (mirror.parentNode !== document.body) {
					$(document.body).append(mirror);
				}

				return this.each(function() {
					var
						ta = this,
						$ta = $(ta),
						maxHeight,
						minHeight,
						boxOffset = 0,
						callback = $.isFunction(options.callback),
						originalStyles = {
							height: ta.style.height,
							overflow: ta.style.overflow,
							overflowY: ta.style.overflowY,
							wordWrap: ta.style.wordWrap,
							resize: ta.style.resize
						},
						timeout,
						width = $ta.width();

					if ($ta.data('autosize')) {
						// exit if autosize has already been applied, or if the textarea is the mirror element.
						return;
