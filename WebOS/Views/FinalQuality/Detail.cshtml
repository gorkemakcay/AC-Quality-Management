								<!-- /.nav nav-pills -->
							</div>
						</div>
					</div>
					<div class="card mt-2 scro" style="min-height:calc(100vh - 150px)">
						<div class="card-body">
							<div class="tab-content">
								<div class="active tab-pane" id="quality">
									@await Component.InvokeAsync("QualityInfo", Model.Id)
								</div>
								<div class="tab-pane" id="specification">
									@await Component.InvokeAsync("QualitySpecifications", Model.Id)
								</div>
								<div class="tab-pane" id="fqControls">
									@await Component.InvokeAsync("QualityFQControls", Model.Id)
								</div>
								<div class="tab-pane" id="explain">
									@await Component.InvokeAsync("QualityExplains", Model.Id)
								</div>
								<div class="tab-pane" id="revisions">
									@await Component.InvokeAsync("QualityRevisions", Model.Id)
								</div>
								<div class="tab-pane" id="documents">
