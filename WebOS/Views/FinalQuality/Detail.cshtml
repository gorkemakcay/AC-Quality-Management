	<script>
		var currentSpecificationsCount = 0;
		var currentSpecificationsModels = [];

		$(document).ready(function() {
			$.ajax({
				type: "GET",
				url: '@Url.Action("GetManuelScenarioInfoJSON", "ManuelScenario")',
				data: { workOrderId: '@Model.WorkOrderNo' },
				success: function(data) {
					model = jQuery.parseJSON(data);
					currentSpecificationsCount = model.length;
					currentSpecificationsModels = model;
					var modelCount = 0;
					var specId = "null";
					var i = 1;
					$("#workOrderNo").val(model[modelCount].WorkOrderId);

					$(model).each(function() {
						specId = 'specification' + i;
						var row = $('<div>', {
							'class': 'row my-3',
						});
						var header = $('<div>', {
							'class': 'form-group col-sm-4'
						}).append($('<input>', {
							'id': specId + 'header',
							'class': 'form-control obs',
							'type': 'text',
							'value': model[modelCount].Entry,
							'style': 'border: none; border-radius: 0px; background-color: #f6f6f6;',
							'disabled': 'true'
						}));
						var observation = $('<div>', {
							'class': 'form-group col-sm-4'
						}).append($('<input>', {
							'id': specId,
							'class': 'form-control obs',
							'type': 'text',
							'value': model[modelCount].Explain,
							'style': 'border: none; border-radius: 0px; background-color: #f6f6f6;',
							'placeholder': 'Açıklama'
						}));
						/////////////////////////////////////////////
						if (model[modelCount].manualControlStatus == 'true') {
							var radioTrue = $('<div>', {
								'class': 'form-group col-sm-1'
							});
							var radioTrueDiv = $('<div>', {
								'class': 'icheck-success d-inline'
							});
							var radioTrueDivInput = $('<input>', {
								'name': 'radio' + i,
								'id': specId + 'radioTrue',
								'type': 'radio',
								'value': 'true',
								'checked': 'true'
							});
							var radioTrueDivLabel = $('<label>', {
								'class': 'mt-2',
								'for': specId + 'radioTrue'
							});
							/////////////////////////////////////////////
							var radioFalse = $('<div>', {
								'class': 'form-group col-sm-1'
							});
							var radioFalseDiv = $('<div>', {
								'class': 'icheck-danger d-inline'
							});
							var radioFalseDivInput = $('<input>', {
								'name': 'radio' + i,
								'id': specId + 'radioFalse',
								'type': 'radio',
								'value': 'false'
							});
							var radioFalseDivLabel = $('<label>', {
								'class': 'mt-2',
								'for': specId + 'radioFalse'
							});
						}
						else if (model[modelCount].manualControlStatus == 'false') {
							var radioTrue = $('<div>', {
								'class': 'form-group col-sm-1'
							});
							var radioTrueDiv = $('<div>', {
								'class': 'icheck-success d-inline'
							});
							var radioTrueDivInput = $('<input>', {
								'name': 'radio' + i,
								'id': specId + 'radioTrue',
								'type': 'radio',
								'value': 'true'
							});
							var radioTrueDivLabel = $('<label>', {
								'class': 'mt-2',
								'for': specId + 'radioTrue'
							});
							/////////////////////////////////////////////
							var radioFalse = $('<div>', {
								'class': 'form-group col-sm-1'
							});
							var radioFalseDiv = $('<div>', {
								'class': 'icheck-danger d-inline'
							});
							var radioFalseDivInput = $('<input>', {
								'name': 'radio' + i,
								'id': specId + 'radioFalse',
								'type': 'radio',
								'value': 'false',
								'checked': 'true'
							});
							var radioFalseDivLabel = $('<label>', {
								'class': 'mt-2',
								'for': specId + 'radioFalse'
							});
						}
						else {
							var radioTrue = $('<div>', {
								'class': 'form-group col-sm-1'
							});
							var radioTrueDiv = $('<div>', {
								'class': 'icheck-success d-inline'
							});
							var radioTrueDivInput = $('<input>', {
								'name': 'radio' + i,
								'id': specId + 'radioTrue',
								'type': 'radio',
								'value': 'true'
							});
							var radioTrueDivLabel = $('<label>', {
								'class': 'mt-2',
								'for': specId + 'radioTrue'
							});
							/////////////////////////////////////////////
							var radioFalse = $('<div>', {
								'class': 'form-group col-sm-1'
							});
							var radioFalseDiv = $('<div>', {
								'class': 'icheck-danger d-inline'
							});
							var radioFalseDivInput = $('<input>', {
								'name': 'radio' + i,
								'id': specId + 'radioFalse',
								'type': 'radio',
								'value': 'false'
							});
							var radioFalseDivLabel = $('<label>', {
								'class': 'mt-2',
								'for': specId + 'radioFalse'
							});
						}

						$("#showSpecifications").append(row);
						row.append(header);
						row.append(observation);
						row.append(radioTrue);
						radioTrue.append(radioTrueDiv);
						radioTrueDiv.append(radioTrueDivInput);
						radioTrueDiv.append(radioTrueDivLabel);
						row.append(radioFalse);
						radioFalse.append(radioFalseDiv);
						radioFalseDiv.append(radioFalseDivInput);
						radioFalseDiv.append(radioFalseDivLabel);
						modelCount++;
						i++;
					});
					if (model != null && model != "") {
						let btnSpecificationUpdate = `<button type="button" onclick="UpdateSpecifications()" class="btn btn-primary mt-2 float-right" style="border:none;border-radius:0px"><i class="far fa-save mr-2"></i>Kaydet</button>`;
						$("#btnQualitySpecifications").html(btnSpecificationUpdate);
					}
				},
				error: function(error) {
					alert("GetManuelScenarioInfoJSON error!");
				}
			});
		});
	</script>

	<script>
		function UpdateSpecifications() {
			var workOrderNo = $("#workOrderNo").val();
			var updateSpecId = 0;
			for (var i = 1; i <= currentSpecificationsCount; i++) {
				updateSpecId = 'specification' + i;

				var updateSpecificationsStatusValue = "null";
				if (document.getElementById(updateSpecId + 'radioTrue').checked) {
					updateSpecificationsStatusValue = document.getElementById(updateSpecId + 'radioTrue').value;
				}
				if (document.getElementById(updateSpecId + 'radioFalse').checked) {
					updateSpecificationsStatusValue = document.getElementById(updateSpecId + 'radioFalse').value;
				}
				var updateSpecificationsModel = {
					Id: currentSpecificationsModels[i - 1].Id,
					Entry: $("#" + updateSpecId + 'header').val(),
					Explain: $("#" + "specification" + i).val(),
					manualControlStatus: updateSpecificationsStatusValue,
					WorkOrderId: workOrderNo
				};

				$.ajax({
					type: "POST",
					url: '@Url.Action("UpdateManuelScenarioJSON", "ManuelScenario")',
					data: { model: updateSpecificationsModel },
					success: function(data) {
					},
					error: function() {
						alert("UpdateManuelScenarioJSON Error!");
					}
				});
			}
			window.location.reload();
			history.go(0);
			window.location.href = window.location.href;
		}
	</script>

