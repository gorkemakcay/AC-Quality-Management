									<!-- /.nav-item -->
									<li class="nav-item"><a class="nav-link" style="border:none;border-radius:0px;font-weight:500" href="#explain" data-toggle="tab">Açıklamalar</a></li>
									<!-- /.nav-item -->
									<li class="nav-item"><a class="nav-link" style="border:none;border-radius:0px;font-weight:500" href="#revisions" data-toggle="tab">Revizyon Bilgisi</a></li>
									<!-- /.nav-item -->
									<li class="nav-item"><a class="nav-link" style="border:none;border-radius:0px;font-weight:500" href="#documents" data-toggle="tab">Dosyalar</a></li>
									<!-- /.nav-item -->
									<li class="nav-item"><a class="nav-link" style="border:none;border-radius:0px;font-weight:500" href="#approval" data-toggle="tab">Onaylar</a></li>
									<!-- /.nav-item -->
